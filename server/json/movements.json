{

    "spreadsheetName": "movements",
    "sheets": [{
            "sheetName": "documents",
            "fields": ["code_doc", "code_part", "type_doc",
                "creation_date", "due_date", "register_date",
                "internal_reference", "code_store",
                "has_movements", "has_payments",
                "credit_value", "debit_value",
                "notes", "partner_notes", "type_movement"
            ],
            "fields_types": ["", "", "", "d", "d", "d", "", "", "b", "b", "n", "n", "", "", ""],
            "hidden_fields": ["type_movement"],
            "entities": [{
                    "entityName": "Invoice",
                    "ukey": "code_doc",
                    "relations": ["invoiceline"],
                    "hidden_fields": ["credit_value", "debit_value"]
                },
                {
                    "entityName": "Receivable",
                    "ukey": "code_doc",
                    "hidden_fields": ["debit_value"]
                },
                {
                    "entityName": "Purchase",
                    "ukey": "code_doc",
                    "relations": ["purchaseline"],
                    "hidden_fields": ["credit_value", "debit_value"]
                },
                {
                    "entityName": "Payment",
                    "ukey": "code_doc",
                    "hidden_fields": ["credit_value"]
                }
            ]

        }, {
            "sheetName": "document_lines",
            "fields": ["code_doc", "code_art", "code_store",
                "price_in", "price_out", "qty_in", "qty_out",
                "discount", "move_date", "notes", "has_movements"
            ],
            "fields_types": ["", "", "", "n", "n", "n", "n", "n", "d", "", "b"],
            "hidden_fields": ["has_movements", "move_date"],
            "entities": [{
                    "entityName": "InvoiceLine",
                    "hidden_fields": ["price_in", "qty_in"]
                },
                {
                    "entityName": "PurchaseLine",
                    "hidden_fields": ["price_out", "qty_out"]
                }
            ]
        },
        {
            "sheetName": "art_inventory",
            "isView": true,
            "formula": "QUERY(document_lines!A:Z,\"select D, E, SUM(H), SUM(I) WHERE D <> '' AND M = true group by D, E label SUM(H) 'qty_in', SUM(I) 'qty_out', SUM(H)-SUM(I) 'qty_net'\", 1)",
            "fields": ["code_art", "code_store", "qty_in", "qty_out", "qty_net"],
            "fields_types": ["", "", "n", "n", "n"],
            "entities": [{
                "entityName": "ArticleInventory"
            }]

        },
        {
            "sheetName": "accounts_inventory",
            "isView": true,
            "formula": "QUERY(documents!A:Z,\"select D, SUM(N), SUM(M) WHERE D <> '' AND L = TRUE group by D label SUM(M) 'credit_value', SUM(N) 'debit_value', SUM(N)-SUM(M) 'balance'\", 1)",
            "fields": ["code_part", "debit_value", "credit_value", "balance"],
            "fields_types": ["", "n", "n", "n"],
            "entities": [{
                "entityName": "AccountInventory"
            }]

        }
    ]
}